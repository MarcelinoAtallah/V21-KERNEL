{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
      "analytics": "2fd1ca1c-1bfb-44bf-b765-c6b8204e2ec5"
    },
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "Valoores-AngularJS": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/Valoores-AngularJS",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "./src/theme.scss",
                            "./node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "./node_modules/ag-grid-community/styles/ag-grid.css",
                            "./node_modules/animate.css/animate.css",
                            "./node_modules/codemirror/lib/codemirror.css",
                            "./node_modules/codemirror/addon/hint/show-hint.css",
                            "./src/assets/styles.css",
                            "./src/assets/styles2.css",
                            "node_modules/leaflet/dist/leaflet.css",
                            "./src/assets/common.css",
                            "./node_modules/leaflet-measure-path/leaflet-measure-path.css",
                            "node_modules/bootstrap/scss/bootstrap.scss"
                        ],
                        "scripts": [
                            "./node_modules/highcharts/highcharts.js",
                            "./node_modules/heatmap.js/build/heatmap.js",
                            "./src/assets/vendors/fontawesome6/js/regular.js",
                            "./src/assets/vendors/fontawesome6/js/solid.js",
                            "./src/assets/vendors/fontawesome6/js/brands.js",
                            "./src/assets/vendors/fontawesome6/js/fontawesome.js",
                            "./node_modules/codemirror/lib/codemirror.js",
                            "./node_modules/codemirror/addon/hint/show-hint.js",
                            "./node_modules/codemirror/addon/hint/sql-hint.js",
                            "./node_modules/codemirror/mode/vb/vb.js",
                            "./src/assets/app.js",
                            "node_modules/jquery/dist/jquery.min.js"

                        ],
                        "allowedCommonJsDependencies": [
                          "codemirror",
                          "jsrender",
                          "highcharts",
                          "highcharts/highcharts-gantt",
                          "highcharts/highmaps",
                          "highcharts/highstock",
                          "codemirror/mode/css/css.js",
                          "codemirror/mode/htmlmixed/htmlmixed.js",
                          "codemirror/mode/javascript/javascript",
                          "codemirror/addon/hint/show-hint",
                          "codemirror/addon/hint/sql-hint",
                          "codemirror/lib/codemirror",
                          "codemirror/mode/sql/sql",
                          "codemirror/mode/vb/vb",
                          "ag-grid-community",
                          "sha.js",
                          "ag-grid-enterprise",
                          "heatmap.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "15mb",
                                    "maximumError": "20mb"
                                }
                          
                            ],
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                          "buildTarget": "Valoores-AngularJS:build:production"
                        },
                        "development": {
                          "buildTarget": "Valoores-AngularJS:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                      "buildTarget": "Valoores-AngularJS:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "./src/theme.scss",
                            "./node_modules/highcharts/css/highcharts.css",
                            "./node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "./node_modules/ag-grid-community/styles/ag-grid.css",
                            "./node_modules/animate.css/animate.css",
                            "./node_modules/codemirror/lib/codemirror.css",
                            "./node_modules/codemirror/addon/hint/show-hint.css",
                            "./src/assets/styles.css",
                            "./src/assets/styles2.css"
                            
                        ],
                        "scripts": [
                            "./node_modules/highcharts/highcharts.js",
                            "./node_modules/heatmap.js/build/heatmap.js",
                            "./src/assets/vendors/fontawesome6/js/regular.js",
                            "./src/assets/vendors/fontawesome6/js/solid.js",
                            "./src/assets/vendors/fontawesome6/js/brands.js",
                            "./src/assets/vendors/fontawesome6/js/fontawesome.js",
                            "./node_modules/codemirror/lib/codemirror.js",
                            "./node_modules/codemirror/addon/hint/show-hint.js",
                            "./node_modules/codemirror/addon/hint/sql-hint.js",
                            "./node_modules/codemirror/mode/vb/vb.js",
                            "./src/assets/app.js"
                        ]
                    }
                }
            }
        }
    }
}
